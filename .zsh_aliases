# some more ls aliases
alias ls='ls --color=auto'
alias ll='ls -Alh'
alias la='ls -A'
alias lz='ll -Z'

alias ..="cd .."

alias dc="cd"
alias l="ls"

alias dcompose='docker compose'
alias unzipwin='UNZIP="-O CP932" unzip'

alias mkcert='docker run -v "$HOME/secret/:/home/linuxbrew/.local:rw" -v "`pwd`:/home/linuxbrew/mkcert:rw" mypaceshun/mkcert'
